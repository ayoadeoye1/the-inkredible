{
  "name": "thekredibank",
  "version": "1.0.0",
  "description": "backend dev recruitment test",
  "main": "dist/server.js",
  "scripts": {
    "format": "prettier --write .",
    "dev": "concurrently \"npx tsc -w\" \"nodemon dist/server.js\"",
    "dev:fmt": "concurrently \"npx prettier --write .\" \"npx tsc -w\" \"nodemon dist/server.js\"",
    "start": "node dist/server.js",
    "db:ms": "npm run db:migrate && npm run db:seed",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "db:seed": "sequelize-cli db:seed:all --debug",
    "db:seed:undo": "sequelize-cli db:seed:undo:all --debug"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.1",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.5",
    "@types/nodemailer": "^6.4.17",
    "@types/swagger-ui-express": "^4.1.7",
    "@types/yamljs": "^0.2.34",
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "sequelize-cli": "^6.6.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.17.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.12.0",
    "nodemailer": "^6.9.16",
    "redis": "^4.7.0",
    "sequelize": "^6.37.5",
    "swagger-ui-express": "^5.0.1",
    "yamljs": "^0.3.0"
  }
}
